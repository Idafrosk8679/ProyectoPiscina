<form method="post" action="{{ path('usuarios_delete', {'dni': usuario.dni}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ usuario.dni) }}">
    <button class="btn">Delete</button>
</form>
