<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
		<title>
			{% block title %}{% endblock %}
		</title>
		<link rel="icon" type="image/png" href="{{ asset ('logoR.png', 'logos')}}">
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset ('bootstrap.min.css', 'css')}}">
			<link rel="stylesheet" href="{{ asset ('styles.css', 'css')}}">
			<link rel="stylesheet" href="{{ asset ('datatables.min.css', 'css')}}">

			<link rel="stylesheet" href="{{ asset ('main-core.css', 'css')}}">
			<link rel="stylesheet" href="{{ asset ('main-daygrid.css', 'css')}}">
			<link rel="stylesheet" href="{{ asset ('main-timegrid.css', 'css')}}">
			<link rel="stylesheet" href="{{ asset ('main-list.css', 'css')}}">
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
		{% block javascripts %}
			<script src="{{ asset ('jquery-3.4.1.min.js', 'js')}}"></script>
			<script src="{{ asset ('bootstrap.min.js', 'js')}}"></script>
			<script src="{{ asset ('script.js', 'js')}}"></script>
			<script src="{{ asset ('script-app.js', 'js')}}"></script>
			<script src="{{ asset ('script-calendar.js', 'js')}}"></script>
			<script src="{{ asset ('datatables.min.js', 'js')}}"></script>
			<script src="{{ asset ('iconos.js', 'js')}}" crossorigin="anonymous"></script>

			<script src="{{ asset ('main-core.js', 'js')}}"></script>
			<script src="{{ asset ('main-daygrid.js', 'js')}}"></script>
			<script src="{{ asset ('main-timegrid.js', 'js')}}"></script>
			<script src="{{ asset ('main-list.js', 'js')}}"></script>
			<script src="{{ asset ('main-moment.js', 'js')}}"></script>
		{% endblock %}
	</body>
</html>
